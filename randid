import random

class Land():
    def __init__(self, name):
        self.name = name

def create_ids(name, number):
    a = {}
    k = 0
    while k < number:
        key = name + str(random.randint(1,10000))
        if key not in a.keys():
            value = Land("Plains")
            a[key] = value
            k +=1
    print(a)


def main():
    create_ids("Plains", 10)

if __name__ == "__main__":
    main()
